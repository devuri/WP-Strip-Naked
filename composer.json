{
  "name"        : "wecodemore/wpstripnaked",
  "description" : "Strip WordPress to its bare essentials.",
  "keywords"    : [ "wordpress", "plugin", "cms", "bare" ],
  "type"        : "wordpress-plugin",
  "homepage"    : "https://github.com/wecodemore/WP-Strip-Naked",
  "license"     : "MIT",
  "authors"     : [
    {
      "name"     : "Franz Josef Kaiser",
      "email"    : "wecodemore@gmail.com",
      "homepage" : "http://unserkaiser.com",
      "role"     : "Developer"
    }
  ],
  "support" : {
    "issues" : "https://github.com/wecodemore/WP-Strip-Naked/issues",
    "source" : "https://github.com/wecodemore/WP-Strip-Naked"
  },
  "require" : {
    "php"                       : ">=5.4",
    "composer/installers"       : "^1",
    "wcm/git-php-hooks"         : "^1",
    "wcm/git-php-hooks-library" : "^1"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*",
    "mockery/mockery" : "dev-master",
    "10up/wp_mock"    : "dev-master",
    "brain/monkey"    : "dev-master"
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "autoload" : {
    "files" : [ "Bootstrap.php" ],
    "psr-4" : {
      "WCM\\StripNaked\\"        : "src/",
      "WCM\\StripNaked\\Tests\\" : "tests/src/"
    }
  },
  "config" : {
    "bin-dir"             : "bin",
    "optimize-autoloader" : true
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.0.x-dev"
    }
  }
}